{
  "extends": [
    "@rocketseat/eslint-config/react",
    "next/core-web-vitals"
  ],
  "rules": {
    "@typescript-eslint/naming-convention": [
      "error",
      {
        "selector": "variable",
        "format": [
          "camelCase",
          "snake_case"
        ]
      },
      {
        "selector": "function",
        "format": [
          "camelCase",
          "snake_case"
        ]
      },
      {
        "selector": "property",
        "format": [
          "camelCase",
          "snake_case"
        ]
      }
    ]
  }
}